{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/view/(.*)",
            "dest": "/server.js",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Cache-Control": "public, max-age=0, must-revalidate"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/server.js",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
} 